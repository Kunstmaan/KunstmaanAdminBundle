<h1>{{ 'Choose a new password' |trans }}</h1>

<form action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST" class="fos_user_resetting_reset">
    {{ form_widget(form) }}
    <div class="actions">
        <input class="btn primary type" type="submit" value="{{ 'Reset password' | trans }}" />
        <input onclick="window.location = '{{ path('fos_user_security_login' ) }}'" type="reset" class="btn" value="{{ 'form.cancel' | trans }}" />
    </div>
</form>
