    <a href="{{ path('KunstmaanAdminBundle_pages_edit', { 'id': node.id }) }}">{{ node.title }}</a>
    <ul>
        {% for node in node.children %}
            {% include 'KunstmaanAdminBundle:Pages:rectreeview.html.twig' %}
        {% endfor %}
    </ul>
</li>