<ul class="db-tabs">
    {% for overviewItem in overviews %}
        <li class="db-tabs__item {% if overviewItem.id == overview.id %}db-tabs__item--active{% endif %}" id="tab{{ overviewItem.id }}">
            <a href="#" class="db-tabs__controller" id="{{ overviewItem.id }}" path="{{ path('KunstmaanAdminBundle_analytics_overview_ajax', {'id':overviewItem.id}) }}">
                <h6 class="db-tabs__item__title">{{ overviewItem.title }}</h6>
                <p class="db-tabs__item__number">{{ overviewItem.visits }}</p>
                <p class="db-tabs__item__subtitle">visitors</p>
            </a>
        </li>
    {% endfor %}
    <div id="lastUpdate">
        {# <a href="#" id="updateButton" class="btn btn-primary"
            data-update-text=" {{ 'analytics.dashboard.update' | trans }}"
            data-updating-text=" {{ 'analytics.dashboard.updating' | trans }}">
            {{ 'analytics.dashboard.lastUpdate' | trans }} {{ lastUpdate }}
        </a> #}

        {{ 'analytics.dashboard.lastUpdate' | trans }} {{ lastUpdate }}
    </div>
</ul>

